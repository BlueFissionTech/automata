{
    "name": "bluefission/automata",
    "description": "Machine learning / communication systems",
    "license": "MIT",
    "keywords": ["automata, ai, nlp, machine learning, chatbot, bot, communication, natural language processing"],
    "authors": [
        {
            "name": "Devon Scott",
            "email": "dscott@bluefission.com",
            "homepage": "https://bluefission.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bluefissiontech/develation"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bluefission/develation": "^1.2.0-alpha",
        "google-gemini-php/client": "*",
        "orhanerday/open-ai": "^2.2",
        "php-ai/php-ml": "^0.10.0",
        "symfony/http-client": "7.4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1@dev",
        "php": ">=8.0.0"
    },
    "suggest": {
        "ext-ds": "Improves performance for collection types (Deque, Map, Set, Stack, PriorityQueue, Vector). Optional; Automata falls back when absent."
    },
    "autoload": {
        "psr-4": {
            "BlueFission\\": "src/"
        },
        "classmap": [
           "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": { 
            "BlueFission\\Tests\\": "tests/" 
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
